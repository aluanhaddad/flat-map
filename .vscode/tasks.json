{
  "version": "2.0.0",
  "command": "npm",
  "type": "shell",
  "tasks": [
    {
      "taskName": "build",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "npm",
      "args": [
        "run",
        "build"
      ]
    },
    {
      "command": "npm",
      "args": [
        "test"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "presentation": {
        "echo": false,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "taskName": "test",
      "promptOnClose": false,
      "type": "shell",
      "problemMatcher": {
        "applyTo": "allDocuments",
        "severity": "error",
        "owner": "Tasks",
        "pattern": [
          {
            "regexp": "(^\\s*at:\\sTest\\..*\\s\\((.*):(\\d+):(\\d+)\\)$)+",
            "file": 2,
            "line": 3,
            "column": 4,
            "message": 0,
            "loop": true
          }
        ],
        "fileLocation": "absolute"
      }
    }
  ]
}